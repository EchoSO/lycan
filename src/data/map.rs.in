use id::{Id, HasForgeableId, HasId};
use uuid::Uuid;

#[derive(Clone, Copy, Debug, Serialize, Deserialize)]
pub struct Map {
    id: Id<Map>,
}

impl HasId for Map {
    type Type = Uuid;
}

impl HasForgeableId for Map {}

impl Map {
    pub fn new(id: Id<Map>) -> Map {
        Map {
            id: id,
        }
    }

    pub fn get_id(&self) -> Id<Map> {
        self.id
    }
}

